{
  "manifest_version": 2,

  "name": "Vulkan Tweaks",
  "version": "0.2.0",
  "description": "Cool features of vulkan generated lesson plans",
  "permissions": [
    "storage",
    "tabs"
  ],

  "content_scripts": [{
    "js": ["content.js"],
    "matches": ["https://*/*", "http://*/*"]
  }],
  "browser_action": {
    "default_popup": "popup.html",
    "default_title": "Vulkan Tweaks"
  }
}
